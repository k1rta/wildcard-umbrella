{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2 --detectOpenHandles --forceExit",
    "test:update": "jest -u",
    "test:related": "jest --findRelatedTests",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint && npm run format",
    "ci": "npm run type-check && npm run lint && npm run build",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "jest --findRelatedTests --passWithNoTests --coverage=false --bail"
    ],
    "*.{json,md,yml,yaml,css}": "prettier --write"
  },
  "dependencies": {
    "@tsparticles/react": "^3.0.0",
    "framer-motion": "^12.23.24",
    "geist": "^1.5.1",
    "lucide-react": "^0.555.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsparticles": "^3.9.1",
    "tsparticles-engine": "^2.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "autoprefixer": "^10.4.17",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.35",
    "prettier": "^3.7.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
