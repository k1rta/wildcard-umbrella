    "lucide-react": "^0.556.0",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.3.7",
    "tailwind-merge": "^3.4.0",
    "tsparticles": "^3.9.1",
    "tsparticles-engine": "^2.12.0"
